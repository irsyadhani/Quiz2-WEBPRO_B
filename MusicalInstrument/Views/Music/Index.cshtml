@model IEnumerable<MusicalInstrument.Entities.Music_Instrument>

@{
    ViewBag.Title = "List of Music Instrumental";
}

<h2>List of Music Instrumental</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.InstrumentCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InstrumentName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.InstrumentCode)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InstrumentName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { code=item.InstrumentCode }) |
            @Html.ActionLink("Details", "Details", new { code=item.InstrumentCode }) |
            @Html.ActionLink("Delete", "Delete", new { code=item.InstrumentCode })
        </td>
    </tr>
}

</table>
